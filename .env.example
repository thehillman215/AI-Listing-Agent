# Copy to .env and fill in your keys
OPENAI_API_KEY=your_openai_api_key_here

# Session
SESSION_SECRET=change_me_securely

# Stripe (enable credits & checkout)
STRIPE_SECRET_KEY=sk_test_xxx
STRIPE_WEBHOOK_SECRET=whsec_xxx

# Stripe price IDs (Dashboard → Products → Prices)
CREDIT_PRICE_20=price_xxx_20
CREDIT_PRICE_50=price_xxx_50
CREDIT_PRICE_200=price_xxx_200

# URLs for Stripe Checkout redirects
STRIPE_SUCCESS_URL=http://localhost:3000/?success=true
STRIPE_CANCEL_URL=http://localhost:3000/?canceled=true

# App
PORT=3000
MODEL_NAME=gpt-5
USE_RESPONSES_API=true
FREE_CREDITS_GUEST=3
FREE_CREDITS_EMAIL=5
RATE_LIMIT_PER_MIN=10


# Email (optional for 'Email my results')
RESEND_API_KEY=re_xxx
SENDER_EMAIL=Listings <no-reply@yourdomain.com>
